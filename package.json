{
  "name": "loja-sapatos-backend",
  "version": "1.0.0",
  "main": "dist/server.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/server.ts --out-dir dist --dts false",
    "start": "node dist/server.js",
    "dev": "dotenv -e .env tsx src/server.ts",
    "start:watch": "dotenv -e .env tsx watch src/server.ts",
    "start:dist": "npm run dist && node dist/src/index.js",
    "seed": "dotenv -e .env tsx src/scripts/seed.ts"
  },
