  "dependencies": {
    "@types/express": "^5.0.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "mongoose": "^8.19.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/mongoose": "^5.11.97",
    "@types/pg": "^8.15.5",
    "dotenv-cli": "^10.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
